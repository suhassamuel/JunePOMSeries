            steps{
            echo("Deploy on QA")
            }
            
        }
        stage("Regression testing"){
            steps{
            echo("Regression testing")
            }
            
        }
          stage("Deply to stage"){
            steps{
            echo("Deply to stage")
            }
        }
        
        stage("Deply to prod"){
            steps{
            echo("Deply to prod")
            }
        }
    }
}